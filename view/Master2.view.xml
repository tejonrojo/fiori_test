<mvc:View controllerName="net.holcim.la.crd1.view.Master2" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m">
	<Page id="master2Page" showNavButton="true" navButtonPress="onNavBack" title="{i18n>master2Title}">
		<subHeader>
			<Bar id="master2SearchBar">
				<contentMiddle>
					<SearchField id="master2SearchField" showRefreshButton="{device>/isNoTouch}" placeholder="{i18n>master2SearchPlaceholder}" search="onSearch"
						tooltip="{i18n>master2SearchTooltip}" width="100%"></SearchField>
				</contentMiddle>
			</Bar>
		</subHeader>
		<content>
			<List id="master2List" items="{ToConversionFactors}" mode="{device>/listMode}" noDataText="{i18n>masterListNoDataText}" select="onSelect"
				growing="true" growingScrollToLoad="true">
				<items>
					<ObjectListItem id="master2ListItem" type="{device>/listItemType}" press="onSelect" title="{ProductId}" number="" numberUnit="">
						<attributes>
							<ObjectAttribute text="{Numerator}"/>
						</attributes>
						<firstStatus>
							<ObjectStatus text="{Denominator}"/>
						</firstStatus>
					</ObjectListItem>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>